{
  "private": true,
  "name": "qev3-config-app",
  "description": "Configuration / Visualization app for QUT Motorsport's second electronic vehicle RaceCar",
  "version": "1.0.0",
  "author": "CallumJHays <callumjhays@gmail.com>, Alex Lossberg",
  "main": "app/background.js",
  "scripts": {
    "dev": "next react",
    "dev:electron": "WEBPACK_TARGET=electron-renderer nextron",
    "build-python": "shx rm -rf pythondist && pyinstaller python/api.py --distpath pythondist --workpath build-py-temp --onefile && shx rm -rf build-py-temp && shx rm api.spec",
    "build:win64": "npm run build-python && nextron build --win --x64",
    "build:win32": "npm run build-python && nextron build --win --ia32",
    "build:mac": "npm run build-python && nextron build --mac --x64",
    "build:linux": "npm run build-python && nextron build --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@jupyter-widgets/base": "2.0.2",
    "@jupyter-widgets/html-manager": "0.18.4",
    "@jupyterlab/services": "4.2.0",
    "@lumino/widgets": "1.10.0",
    "@projectstorm/react-workspaces": "2.2.2",
    "antd": "3.26.7",
    "closure-webpack-plugin": "^2.3.0",
    "cross-spawn": "6.0.5",
    "crossfilter2": "^1.5.2",
    "electron-serve": "0.4.1",
    "electron-store": "5.1.0",
    "everpolate": "0.0.3",
    "geojson": "0.5.0",
    "google-closure-compiler": "^20200315.0.0",
    "isomorphic-unfetch": "3.0.0",
    "iterare": "^1.2.0",
    "next-transpile-modules": "3.0.2",
    "plotly.js": "1.52.1",
    "react-plotly.js": "2.4.0",
    "runtypes": "4.1.0",
    "uuid": "7.0.1"
  },
  "devDependencies": {
    "@svgr/webpack": "5.1.0",
    "@types/cross-spawn": "6.0.0",
    "@types/geojson": "7946.0.7",
    "@types/ip": "1.1.0",
    "@types/node": "12.12.21",
    "@types/react": "16.9.17",
    "@types/react-color": "3.0.1",
    "@types/react-dom": "16.9.4",
    "@types/react-plotly.js": "2.2.4",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "babel-plugin-import": "1.13.0",
    "dotenv": "8.2.0",
    "electron": "7.1.7",
    "electron-builder": "21.2.0",
    "electron-packager": "9.0.1",
    "electron-rebuild": "1.8.5",
    "ip": "1.1.5",
    "less": "3.10.3",
    "next": "9.3.2",
    "nextron": "5.14.3",
    "null-loader": "3.0.0",
    "react": "16.12.0",
    "react-color": "2.18.0",
    "react-dom": "16.12.0",
    "shx": "0.3.2",
    "typescript": "3.7.3"
  }
}
