{
  "private": true,
  "name": "qev2-config",
  "description": "Configuration / Visualization app for QUT Motorsport's second electronic vehicle racecar",
  "version": "1.0.0",
  "author": "CallumJHays <callumjhays@gmail.com>, Alex Lossberg",
  "main": "app/background.js",
  "scripts": {
    "dev": "next",
    "dev:electron": "nextron",
    "build-python": "shx rm -rf pythondist && pyinstaller python/api.py --distpath pythondist --workpath build-py-temp --onefile && shx rm -rf build-py-temp && shx rm api.spec",
    "build:win64": "npm run build-python && nextron build --win --x64",
    "build:win32": "npm run build-python && nextron build --win --ia32",
    "build:mac": "npm run build-python && nextron build --mac --x64",
    "build:linux": "npm run build-python && nextron build --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "cross-spawn": "^6.0.5",
    "electron-serve": "^0.4.1",
    "electron-store": "^5.1.0",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/superagent": "^4.1.3",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.8.5",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "next": "^9.1.6",
    "nextron": "^5.14.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "shx": "^0.3.2",
    "typescript": "^3.7.3"
  }
}
